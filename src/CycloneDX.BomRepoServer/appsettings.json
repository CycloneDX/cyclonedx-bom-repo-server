{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AllowedMethods": {
    "Get": false,
    "Post": false,
    "Delete": false
  },
  "Repo": {
    "StorageType": "FileSystem",
    "Options": {
      "Directory": "Repo"
    }
  },
  "Retention": {
    "MaxBomVersions": 0,
    "MaxBomAge": 0
  }
}
